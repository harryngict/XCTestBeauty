{
  "name": "BeautyTest",
  "version": "0.0.1",
  "summary": "BeautyTest module",
  "description": "A swift module to support writing unit test and snapshot test.",
  "homepage": "https://github.com/harryngict/BeautyTest",
  "source": {
    "git": "git@github.com:harryngict/BeautyTest.git",
    "tag": "0.0.1"
  },
  "authors": {
    "Hoang Nguyen": "harryngict@gmail.com"
  },
  "license": {
    "type": "MIT",
    "text": "Copyright (c) 2023"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "static_framework": true,
  "subspecs": [
    {
      "name": "XCShortHand",
      "source_files": "BeautyTest/XCShortHand/**/*.{swift,h,m}",
      "frameworks": [
        "Foundation",
        "XCTest"
      ]
    },
    {
      "name": "SnapshotTest",
      "source_files": "BeautyTest/SnapshotTest/**/*.{swift,h,m}",
      "frameworks": [
        "Foundation",
        "XCTest"
      ],
      "dependencies": {
        "iOSSnapshotTestCase": [
          "~> 8.0.0"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
